<script>
		/*头部加载==============================================*/
		$('.perHead').load('../head.html');
		/*学期选择 模拟select===================================*/
		$('.semester_tc').on('click',function(e){
			$(this).find('ul').toggle();
		});
		$('.semester_tc>ul li').on('click',function(){
			$(this).addClass('active')
			.siblings().removeClass('active')
			.parent().siblings('p').children('span').text($(this).text());
		});
		/*班级点击切换效果===================================*/
		$('.grade a').on('click',function(){
			$(this).addClass('active').siblings().removeClass('active');
		});
		//table
		$('#fraction_table').DataTable({
			paging: false, // 不分页
			info: false, // 不显示info
			searching: false, // 不显示搜索框 
			language: {
				emptyTable: "暂无数据"
			},
			"columnDefs": [
                { "type": "natural", targets: [2,4,5,6,7,8,9,10] },
                { "type": "chinese-string", targets: 1},
                {"type":"defineNum",targets: 3}
            ]

		});
   		//滚动
	    $('#boxscroll').niceScroll({
		    cursorcolor: "#ccc",//光标颜色
		    cursoropacitymax: 1, //改变不透明度非常光标处于活动状态（scrollabar“可见”状态），范围从1到0
		    touchbehavior: false, //使光标拖动滚动像在台式电脑触摸设备
		    cursorwidth: "5px", //像素光标的宽度
		    cursorborder: "0", // 游标边框css定义
		    cursorborderradius: "5px",//以像素为光标边界半径
		 	background: "#f1f1f1",//轨道背景颜色
		    autohidemode: false //是否隐藏滚动条
		});
</script>