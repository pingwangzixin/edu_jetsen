<!-- Swiper 初始化-->
<script>
    var swiper = new Swiper('.swiper-container', {
    pagination: '.swiper-pagination',
    nextButton: '.swiper-button-next',
    prevButton: '.swiper-button-prev',
    paginationClickable: true,
    spaceBetween: 30,
    centeredSlides: true,
    autoplay:2000,
    autoplayDisableOnInteraction: false
});
</script>

<script>
    $(function(){
        $(".dw3").on("click",function(){
            $(".strlist").toggle(300)
        })
        
        $(".strlist ul li span").on("click",function(){
            $(this).addClass("nameactive").parent().siblings().children("span").removeClass("nameactive")
        })
    })
    
    function andJump(){
        window.android.AndJump();
    }

    function andJump2(){
        window.android.AndJump2();
    }
</script>


<script>
    $(function(){
         var str = window.location.search;
        var name=str.split('=')

        $(".wodename span").html(decodeURI(name[1]));
    })
    
</script>

