	<script type="text/javascript">
		$(function () {
			//控制x轴上最后的数字
		    var kd = 1;
		    var maxScore=97;
		    if(maxScore%2==0){
		     	kd = 2;
		    }
		    if(maxScore%3==0){
		     	kd = 3;
		    }
		    if(maxScore%4==0){
		     	kd = 4;
		    }
		    if(maxScore%5==0){
		     	kd = 5;
		    }
		    if(maxScore%6==0){
		     	kd = 6;
		    }
		    if(maxScore%7==0){
		     	kd = 7;
		    }
		    if(maxScore%8==0){
		     	kd = 8;
		    }
		    if(maxScore%9==0){
		     	kd = 9;
		    }
		    if(maxScore%10==0){
		     	kd = 10;
		    }
			
			
			//动态赋值高度
		    var _Num=20;//学生的个数
		    $('#chengji_box').css('minHeight','150px');
		    $('#chengji_box').css('height',_Num*50+'px');
			
		     $('#chengji_box').highcharts({
		        chart: {
		            type: 'bar',
		            plotBorderColor: '#ccc',
		            plotBorderWidth: 1,
		            alignTicks:false
		        },
		        title: {
		            text: null
		        },
		        subtitle: {
		            text: null
		        },
		        
		        xAxis: {
		            categories: ['学生1', '学生2', '学生3', '学生4', '学生5','学生6', '学生7', '学生8', '学生9', '学生10', '学生11', '学生12', '学生13', '学生14','学生15','学生16', '学生17', '学生18', '学生19', '学生20'],
		            title: {
		                text: null
		            }
		        },
		      
		        yAxis: {
		            min: 0,
		            max:maxScore,
		            tickInterval:kd,
		            title: {
		                text: null,
		            }
		        },
		        tooltip: {
		            valueSuffix: '分'
		        },
		        plotOptions: {
		            bar: {
		                dataLabels: {
		                    enabled: true
		                }
		            }
		        },
		        credits: {
		            enabled: false
		        },
		        series: [{
		            name: '分数',
		            data: [2,3,45,43,20,20,28,24,26,32,43,58,20,35,20,26,10,25,43,20]
		        }]
		    });
		});
