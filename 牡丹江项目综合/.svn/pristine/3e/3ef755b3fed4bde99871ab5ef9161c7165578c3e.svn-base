.zy_import_part_lower .task_list>li .qusetion_main .edit_state .editor_unified{}

.img_zoom{display: none; position: absolute; top: 100px; left: 0; z-index: 999;}
.img_zoom .img_zoom_btn{position: relative; width: 76px; height: 34px; background: #eaeaea; border-radius: 2px;}
.img_zoom .img_zoom_btn:after{position: absolute; bottom: -12px; left: 50%; margin-left: -6px; content: ''; width: 0; height: 0; border: 6px solid transparent; border-top-color: #eaeaea;}
.img_zoom .img_zoom_btn b{width: 22px; height: 22px; line-height: 22px; text-align: center; font-size: 19px; color: #eaeaea; background: #5093e1; border-radius: 50%; margin: 6px 8px; -moz-user-select: none; -khtml-user-select: none; user-select: none; cursor: pointer;}
.img_zoom .img_zoom_btn b.enlarge{}
.img_zoom .img_zoom_btn b.narrow{}

/*弹框框*/
.public_box{font-size: 14px;}
.public_box .con{box-sizing: border-box; padding: 0 16px;}
.public_box .con .tit{height: 34px; text-align: left; border-bottom: 2px solid #e5e5e5;}
.public_box .con .tit span{display: inline-block; line-height: 33px; font-size: 14px; color: #5093e1; border-bottom: 1px solid #e5e5e5;}
.public_box .con .btn_group{}
.public_box .con .btn_group button{outline: none; width: 110px; height: 30px; border: 1px solid transparent; border-radius: 5px; cursor: pointer;}
.public_box .con .btn_group button.sure{background: #dfecfa; color: #5093e1; margin-right: 20px;} 
.public_box .con .btn_group button.cancel{background: #fff; border-color: #e5e5e5;} 
input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button {-webkit-appearance: none;}
input[type=number]{-moz-appearance:textfield; }  
input[type=number]::-webkit-inner-spin-button,input[type=number]::-webkit-outer-spin-button{-webkit-appearance: none; margin: 0;} 

.public_box .con .main_cont p input{box-sizing: border-box; width: 180px; padding: 0 10px; height: 30px; border: 1px solid #e5e5e5; border-radius: 5px;}
.public_box .con .main_cont p.tip{display: none; text-align: center; color: #D63E3E; margin-top: 2px;}

