.main_right .list_tit li a.list_tit_active{border-bottom:2px solid #5093e1;color:#222}
.main .mainLeft {margin-bottom: -99999999px; padding-bottom: 99999999px; overflow: hidden;}

.zy_main_part{font-size: 14px;}
.zy_main_part .main_right{padding-bottom: 20px;}
.zy_main_part .list_tit li:last-of-type{float: right;}
.zy_main_part .list_tit li:last-of-type a{color: #5093e1;}
.zy_import_part{margin-top: 10px;}
.zy_import_part_upper{background: #fff; padding: 20px; border-radius: 5px;}
.zy_import_part_upper .tit{padding-bottom: 6px; border-bottom: 1px solid #e5e5e5;}
.zy_import_part_upper .tit span{font-size: 16px; line-height: 30px; color: #5093e1;}
.zy_import_part_upper .tit button{padding: 0 26px; height: 30px; background: #5093e1; color: #fff; border-radius: 5px; outline: none; border: none; cursor: pointer;}
.zy_import_part_upper>ul{}
.zy_import_part_upper>ul li{margin-top: 20px;}
.zy_import_part_upper>ul li>b{color: #ff0000;}
.zy_import_part_upper>ul li>select{height: 30px; padding: 0 10px; margin-right: 20px; border: 1px solid #e5e5e5; border-radius: 5px; outline: none;}
.zy_import_part_upper>ul li>input[type=text]{box-sizing: border-box; width: 320px; height: 30px; padding: 0 10px; border: 1px solid #e5e5e5; border-radius: 5px; outline: none;}

.zy_import_part_lower{background: #fff; padding: 20px 20px 50px; margin-top: 16px; border-radius: 5px;}
.zy_import_part_lower .tit{padding-bottom: 6px; margin-bottom: 16px; border-bottom: 1px solid #e5e5e5;}
.zy_import_part_lower .tit span{font-size: 16px;}
.zy_import_part_lower .tit ul{}
.zy_import_part_lower .tit ul li{float: left; color: #5093e1; margin-left: 30px; cursor: pointer;}

.zy_import_part_lower .task_list{position: relative;}
.zy_import_part_lower .task_list>li{position: relative; padding: 22px 10px 10px; margin-bottom: 10px; border: 1px solid transparent;}
.zy_import_part_lower .task_list>li.active{border-color: #999;}
.zy_import_part_lower .task_list>li>.btn{display: none; position: absolute; top: 0; right: 0; height: 22px; line-height: 22px; background: #e5e5e5; color: #5093e1;}
/*.zy_import_part_lower .task_list>li:hover .btn:first-of-type{display: block;}*/
.zy_import_part_lower .task_list>li>.btn b{display: inline-block; width: 70px; margin: 0 2px; text-align: center; font-weight: normal; cursor: pointer;}
.zy_import_part_lower .task_list>li .question_option{margin-bottom: 10px;}
.zy_import_part_lower .task_list>li .question_option .question_type{line-height: 30px;}
.zy_import_part_lower .task_list>li .question_option .question_type>b{font-weight: normal;}
.zy_import_part_lower .task_list>li .question_option .question_type select{height: 30px; padding: 0 10px; margin-left: 6px; border: 1px solid #e5e5e5; border-radius: 5px; outline: none; cursor: pointer;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template{max-width: 84%; margin-left: 16px;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template>span{display: none;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem{display: none; max-width: 96%; min-width: 750px;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul{margin: 0 10px;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul li:not(.btn){float: left; box-sizing: border-box; width: 28px; height: 28px; line-height: 28px; text-align: center; margin: 0 4px 4px; border: 1px solid #3f3f3f; border-radius: 50%; cursor: pointer;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul li.active{color: #fff; background: #5093e1; border-color: transparent;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul li.btn{margin-left: 10px;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul li.btn img{margin: 0 2px;}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .all_tem ul li.btn img{vertical-align: middle; cursor: pointer;}

.zy_import_part_lower .task_list>li .question_option .question_type .question_template .single{}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .multiple{}
.zy_import_part_lower .task_list>li .question_option .question_type .question_template .judge{}

.zy_import_part_lower .task_list>li .qusetion_main{}
.zy_import_part_lower .task_list>li .qusetion_main .normal_state{}
.zy_import_part_lower .task_list>li .qusetion_main .normal_state .stem_cont{}
