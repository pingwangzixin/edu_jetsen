<script>
	$(function(){
		$(".pingyuing").on("mouseover mouseout",function(event){
			if(event.type == "mouseover"){
				$(this).parent().find(".pingyutc").show()
			}else if(event.type == "mouseout"){
				$(this).parent().find(".pingyutc").hide()
			}
		})
		
		/*table表格超出有滚动条*/
			function tableScoll(config){
				var boxW=$(config.boxName).width();
				var lengths=$(config.lengthsNmae).length;
				var boxNmae_table=$(config.boxNmae_table);
				if(lengths>config.num && boxW>config.scw){
					lengths-=config.num;
					boxNmae_table.css({"width":config.min_w+lengths*config.tdW});
				}
			}
			tableScoll({
				"boxName":".dafencon",//外盒子宽度
				"lengthsNmae":".dafencon table thead tr:last-child th",//具体的需要增加的数据
				"boxNmae_table":".dafencon table",//当前table表
				"num":2,//设定不能超过的个数
				"scw":1100,//设定不能超过的宽度
				"min_w":1200,//表格最小宽度
				"tdW":220//超过部分每一个表格的宽度
			});
	})
</script>
