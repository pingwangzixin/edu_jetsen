/*放大效果*/
.situation_fixed{
	width: 75%;
	min-width: 1010px;
	max-height: 520px;
	overflow-y: auto;
	overflow-x: hidden;
	position: absolute;
	top:0;
	right:0;
	left:0;
	bottom: 0;
	margin: auto;
	z-index: 999999;	
}
.situation_close{
	position: absolute;
	right: 10px;
    top: 8px;
    z-index:9999999;
    display:none;
    cursor:pointer;
}



/*教研组*/
/*首页*/
.researchGroup{padding-bottom: 0;}
.researchGroup .seach{
	width:217px;
