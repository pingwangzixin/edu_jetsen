.mlh_noborder{
	border:none;
}
.mlh_content{
	margin:0 auto;
	margin-top:16px;
	width:1200px;
	border: none;
}
.res_detail em,
.res_detail i{
	font-style: normal;
	vertical-align: middle;
	line-height: 1;
}
/*领导端  首页左侧菜单*/
.mlh_left{
	float: left;
	width: 178px;
	font-size: 14px;
	color:#282a2c;
	background: #fff;
	border-radius: 5px;
}
.mlh_left .menu{
	padding:10px 0;
	width: 178px;
}
.mlh_border{
	margin: 1px 10px;
	border-bottom: 2px solid #f1f2f3;
}
.mlh_left .menu .item p em{
	display: flex;
	align-items: center;
}
.mlh_left .menu .item p{
	padding:5px 10px;
	height: 40px; 
	line-height: 40px; 
	color: #4f5355;
	cursor: default;
}
.mlh_left .menu .item:not(:last-of-type) p em{
	display: flex; 
	align-items: center; 
}
.mlh_left .menu .item.active p{
	background: #399fdf; 
	color: #fff;
}
.mlh_left .menu .item .nonpoint{
	cursor: default;
}
.mlh_left .menu .item.active p .iconfont{
	color: #fff;
}
.mlh_left .menu em{
	display: block;
	width: 100%;
	font-style: normal;
}
.mlh_left .mlh_borderT{
	border-radius: 5px 5px 0 0;
}
.mlh_left .mlh_borderB{
	border-radius:0 0 5px 5px;
}
.mlh_left .menu .mlh_noborder{
	border-top:none;
}
.mlh_left .menu > .item span{
	flex: 2;
	padding-left: 10px;
	display: inline-block;
	text-align: left;
}
.mlh_left .menu > .item .iconfont{
	padding-left: 10px;
	font-size: 18px;
	color: #414345;
}
.mlh_left .sub_menu li{
	height: 33px; 
	line-height: 33px; 
	text-align: center;
}
.mlh_left .sub_menu > li span{
	height: 100%;
	cursor: pointer;
}
.mlh_left .sub_menu > li:hover{
	color: #1479bf;
}
.mlh_left .sub_menu li.active{
	background: #399fdf;
	color: #fff;
}
/*领导端 首页右侧tab列表样式 */
.mlh_right{
	float: right;
	width: 1006px;
}
.mlh_right .list{
	width: 100%;
	height: auto;
	min-height: 600px;
	_height:600px;
	padding-bottom: 20px;
	color: #4f5355;
	background: #fff;
	border-radius: 5px;
}
.mlh_right .tab{
	height: 61px;
	border-bottom: 2px solid #f1f2f3;
}
.mlh_right .tab ul{
	height: 60px;
	line-height: 60px;
	font-size: 16px;
	border-bottom: 3px solid transparent;
}
.mlh_right .tab ul li{
	position: relative;
	float: left;
	margin: 0 22px;
}
.mlh_right .tab ul li .icon-dian{
	position: absolute;
	top: -6px;
	right: -15px;
	font-size: 24px;
	color: #f97045;
}
.mlh_right .tab_cont ul{
	margin: 0 20px;
}
.mlh_right .tab_cont li{
	display: flex;
	justify-content: space-between;
	height: 50px;
	line-height: 50px;
	font-size: 12px;
	color: #7f8487;
	border-bottom: 1px dashed #e0e2e4;
}
.mlh_right .tab_cont a{
	font-size: 14px;
	color: #282a2c;
}
/*领导端 详情页面*/
.mlh_right .title{
	width: 100%;
	height: 50px;
	line-height: 50px;
	font-size: 16px;
	color: #1470bf;
	background: #fff;
	border-radius: 5px;
}
.mlh_right .title i{
	display: inline-block;
	padding-left: 30px;
	vertical-align: middle;
	font-size: 20px;
}
.mlh_right .pointer{
	cursor: pointer;
}
.mlh_right .details{
	box-sizing: border-box;
	margin-top:10px;
	padding:10px 20px;
	font-size: 14px;
	color: #282a2c;
}
.mlh_right .table {
    display:table;
}
.mlh_right .table_tr {
    display: table-row;
    line-height: 1.5;
}
.mlh_right .table_td {
    display: table-cell;
    padding:8px 0;
}
.mlh_right .table_td:first-child{
	width: 62px;
}
.mlh_right .btn_border{
	margin-right: 10px;
	padding:2px 5px;
	border-radius: 5px;
}
.mlh_right .expert{
	color: #399fdf;
	border:1px solid #399fdf;
}
.mlh_right .teacher{
	color: #27c0a1;
	border:1px solid #27c0a1;
}
.mlh_right .institution{
	display: inline-block;
	padding-right: 20px;
}
.mlh_right .table_td img{
	margin-top: 15px;
} 
.mlh_right .adjunct{
	color: #399fdf;
}
/*教师端 导学列表*/
.mlh_content .guide{
