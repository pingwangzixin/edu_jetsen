    /*日历插件开始*/
.zmj_calendar{width: 402px;height: 408px;background: #fff;border:1px solid  #399fdf;position: absolute;right:-30px;top: 42px;z-index: 222;}
.zmj_calendar .laydate_top{padding: 12px 13px;overflow: hidden;}
.zmj_calendar .laydate_y{width: 73px;height: 34px;padding: 0 14px;border:1px solid #ddd;margin-right: 7px;float: left;cursor: pointer;}
.zmj_calendar .laydate_y input{border:none;width: 60px;line-height: 36px;background: transparent;cursor: pointer;}
.zmj_calendar .laydate_ym label:after{content: "";height: 0;display: inline-block;border: 4px solid transparent;border-top-color: #000 ;cursor: pointer;}
.zmj_calendar .laydate_m{width: 127px;height:34px;border: 1px solid #ddd;float: left;cursor: pointer;}
.zmj_calendar .laydate_m input{width: 50px;text-align: center;line-height: 36px;border: none;background: transparent;cursor: pointer;}
.zmj_calendar .laydate_m .laydate_choose{width: 27px;height: 34px;background: #f1f2f3;display:block;font-size: 14px;text-align: center;line-height: 34px;}
.zmj_calendar .laydate_chprev{float: left;}
.zmj_calendar .laydate_chnext{float: right;}
.zmj_calendar .laydate_back{width:93px;height: 34px;font-size: 14px;text-align: center;line-height: 34px;background:#f1f2f3;border:1px solid #ddd;margin-left: 40px;float: left;cursor: pointer;}
.zmj_calendar .laydate_yms{width: 150px;text-align: center;position: absolute;left:12px;background:#fff;border:1px solid #ccc;z-index: 111;}
 #laydate_ms{left: 124px;}
.zmj_calendar .laydate_show .laydate_tab{width: 100%;height:30px;display: block;clear: both;}
.zmj_calendar .laydate_show .laydate_chtop  cite:after{content:"";height: 0;border:6px solid transparent;border-bottom-color: #000;display: inline-block;position: relative;top:6px;}
.zmj_calendar .laydate_show .laydate_chdown cite:after{content:"";height: 0;border:6px solid transparent;border-top-color: #000;display: inline-block;position: relative;top:12px;}
.zmj_calendar .laydate_show ul li{line-height: 30px;width: 75px;line-height: 30px;text-align: center;float: left;cursor: pointer;}
.zmj_calendar .laydate_m .laydate_yms span{width:75px;height: 30px;text-align: center;line-height: 30px;display: block;float: left;}
.zmj_calendar .laydate_yms, .zmj_calendar .laydate_time { display: none;}
.zmj_calendar .laydate_show {display: block;}
.zmj_calendar .laydate_yms .laydate_click{background: #ccc;}
.zmj_calendar .laydate_table{margin:0 13px;border-top:1px solid #399fdf;display: block;border-collapse: collapse;}
.zmj_calendar .laydate_table tr td,.laydate_table tr th{width: 54px;height: 48px;font-size: 14px;color: #282a2c;text-align: center;line-height: 48px;border-bottom:1px solid #ddd;}
.zmj_calendar .laydate_table tr td{position: relative;cursor:default;}
.zmj_calendar .laydate_table tr td span{width: 52px;height:36px;font-size: 14px;color:#fff;text-align: center;line-height: 36px;background-color: rgba(0, 0, 0, 0.8);opacity: 0.8;border-radius: 4px;display: block;position: absolute;left: 0;top:-36px; display: none;}
.zmj_calendar .laydate_table tr td span:after{content:" ";height: 0;border:6px solid transparent;border-top-color: #000;display:inline-block;position: absolute;top:36px;left:20px;opacity: 0.8;}
.zmj_calendar .laydate_table tr:last-child td{border: none;}
.zmj_calendar .laydate_table .laydate_nothis{color: #7f8487}
.zmj_calendar .laydate_table td.laydate_click{position: relative;}
.zmj_calendar .laydate_table td.laydate_click:after{content: "";width: 28px;height: 28px;background:#f97046;border-radius: 100%;display: block;position:absolute;top: 10px;left:13px;opacity: 0.6;}
.zmj_calendar .laydate_table .active{position: relative;cursor: pointer;}
.zmj_calendar .laydate_table .active:before{content: "";width: 6px;height: 6px;background:#f97046;border-radius: 100%;display: inline-block;position: absolute;left:24px;top:36px;}
.zmj_calendar .laydate_table .active:hover span{display:block;}
.zmj_calendar .laydate_table td b{font-weight: normal;}
.zmj_calendar .laydate_btn{display: none;}
