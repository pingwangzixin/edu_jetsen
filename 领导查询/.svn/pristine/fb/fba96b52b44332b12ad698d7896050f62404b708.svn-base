<!--资源管理列表 详情 start-->
<div class="mlh_right mlh_width" ng-if="variablePacket.page == 'secondNav.leftTree.resourceManage.resourceManageList'">
<div class="mlh_resdetails">
	<div class="zy_guide_tit clearfix">
		<div class="fl mlh_rescont">
			<ul class="title_left">
				<li>
					<i class="iconfont icon-gongxiangtubiaozhuangtaileicaozuolei59" 
						ui-sref="secondNav.leftTree.resourceManage.resourceManageList" 
						ui-sref-opts="{reload:true}"
						ng-click="back()" ></i>
					<span ng-bind="resouce.title"></span>
					<span class="fine" ng-if="variablePacket.fine">校精品</span>
				</li>
				<li class="res_detail mlh_detail">
					<span class="res_name">
						<img ng-src="{{resouce.fileImage}}"/>
						<em>{{resouce.createUser}}</em>
					</span>
					<span class="res_site">
						<i class="iconfont icon-location"></i>
						<em>{{resouce.areaName}}</em>
					</span>
					<span class="res_time">
						<i class="iconfont icon-lishi"></i>
						<em>{{resouce.createDate}}</em>
					</span>
					<span class="res_browse">
						<i class="iconfont icon-kejian"></i>
						<em>{{resouce.browse}}</em>
					</span>
					<span class="res_download">
							<i class="iconfont icon-xiazai" ng-click="downloadFile(resouce.id,resouce.title)"></i>
							<em>{{resouce.download}}</em>
						</span>
				</li>
			</ul>
			<a href="javascript:;" ng-click="deleDate1($index,resouce.id)">删除</a>
		</div>
	</div>
