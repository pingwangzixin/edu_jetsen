.zy_archives{}
.zy_archives .title_box{height: 50px; line-height: 50px; padding: 0 16px; background: #2f4048; border-radius: 2px;}
.zy_archives .title_box .name{color: #fff; font-size: 16px;}
.zy_archives .title_box .name b{font-weight: normal;}
.zy_archives .title_box .btn{height: 34px; line-height: 34px; padding: 0 16px; margin-top: 8px; background: #fff; border-radius: 5px; cursor: pointer;}

.zy_archives{}
.zy_archives .cont{margin-top: 16px;}
.zy_archives .cont .left_nav{width: 186px; height: 910px; text-align: center; background: #fff; box-shadow: 0 0 4px 0 #ccc;}
.zy_archives .cont .left_nav p{height: 42px; line-height: 42px; font-size: 18px; color: #fff; background: #5093e1;}
.zy_archives .cont .left_nav ul{font-size: 16px;}
.zy_archives .cont .left_nav ul li{height: 50px; line-height: 50px; cursor: pointer;}
.zy_archives .cont .left_nav ul li.active{background: #f6f6f6; color: #5093e1;}
.zy_archives .cont .right_main{width: 1006px;}
.zy_archives .cont .right_main .pieces{box-shadow: 0 0 4px 0 #ccc;}
.zy_archives .cont .right_main .pieces .tit{height: 42px; line-height: 42px; padding: 0 20px; background: #f5f5f5; border-bottom: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .tit span{color: #5093e1; cursor: pointer;}
.zy_archives .cont .right_main .pieces .tit span a{color: #5093e1;}

.zy_archives .cont .right_main .pieces .personality_list{padding: 14px 25px 20px; background: #fff;}
.zy_archives .cont .right_main .pieces .personality_list li{width: 33.3%;}
.zy_archives .cont .right_main .pieces .personality_list li .time{color: #666;}
.zy_archives .cont .right_main .pieces .personality_list li .details{margin-top: 6px;}
.zy_archives .cont .right_main .pieces .personality_list li .details img{width: 106px; margin-right: 10px;}
.zy_archives .cont .right_main .pieces .personality_list li .details p{width: 190px; color: #333;}
.zy_archives .cont .right_main .pieces .personality_list li .details p b{display: block; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;}
.zy_archives .cont .right_main .pieces .personality_list li .details p span{display: block; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;}
.zy_archives .cont .right_main .pieces .personality_list li .details p strong{color: #999; font-weight: normal;}

.zy_archives .cont .right_main .pieces_two{margin-top: 10px;}
.zy_archives .cont .right_main .pieces_two .pieces:first-child{width: 690px;}
.zy_archives .cont .right_main .pieces_two .pieces:last-child{width: 306px;}
.zy_archives .cont .right_main .pieces_two .achievement{}
.zy_archives .cont .right_main .pieces_two .leave{}

.zy_archives .cont .right_main .pieces .achievement_list{box-sizing: border-box; height: 192px; padding: 14px 20px 20px; background: #fff;}
.zy_archives .cont .right_main .pieces .achievement_list table{width: 100%; border: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .achievement_list table tr{}
.zy_archives .cont .right_main .pieces .achievement_list table th{font-weight: normal; height: 32px; text-align: center; background: #f2f2f2; border: 1px solid #ddd;}
/* .zy_archives .cont .right_main .pieces .achievement_list table tr{} */
.zy_archives .cont .right_main .pieces .achievement_list table td{height: 32px; text-align: center; border: 1px solid #ddd;}

.zy_archives .cont .right_main .pieces .leave_list{padding: 48px 8px 42px; background: #fff;}
.zy_archives .cont .right_main .pieces .leave_list p{box-sizing: border-box; width: 50%; text-align: center; line-height: 34px;}
.zy_archives .cont .right_main .pieces .leave_list p:first-of-type{border-right: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .leave_list p span{display: block;}
.zy_archives .cont .right_main .pieces .leave_list p b{display: block; font-weight: normal;}
.zy_archives .cont .right_main .pieces .leave_list p b.active{color: #ff9000;}
.zy_archives .cont .right_main .pieces .leave_list p em{display: block; font-style: normal;}

.zy_archives .cont .right_main .pieces .healthy_list{box-sizing: border-box; height: 450px; padding: 20px 20px 16px; background: #fff;}
.zy_archives .cont .right_main .pieces .healthy_list ul{padding: 0 20px; margin-bottom: 10px;}
.zy_archives .cont .right_main .pieces .healthy_list ul li{float: left; width: 20%; text-align: center;}
.zy_archives .cont .right_main .pieces .healthy_list ul li i{display: block; color: #35a4e4; font-size: 30px;}
.zy_archives .cont .right_main .pieces .healthy_list ul li b{display: block; font-weight: normal; color: #999; margin: 6px 0;}
.zy_archives .cont .right_main .pieces .healthy_list ul li span{display: block; }
.zy_archives .cont .right_main .pieces .healthy_list ul li span em{font-style: normal; color: #ff9000;}
.zy_archives .cont .right_main .pieces .healthy_list table{width: 100%; border: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .healthy_list table tr{}
.zy_archives .cont .right_main .pieces .healthy_list table th{font-weight: normal; height: 32px; text-align: center; border: 1px solid #ddd;}
/* .zy_archives .cont .right_main .pieces .achievement_list table tr{} */
.zy_archives .cont .right_main .pieces .healthy_list table td{height: 32px; text-align: center; border: 1px solid #ddd;}

.zy_archives .cont .right_main .pieces .activity_list{background: #fff; padding-bottom: 10px;}
.zy_archives .cont .right_main .pieces .activity_list .top{background: #f9f9f9; padding: 8px; border-bottom: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .activity_list .top p{box-sizing: border-box; width: 50%; text-align: center; padding-bottom: 8px;}
.zy_archives .cont .right_main .pieces .activity_list .top p:first-of-type{border-right: 1px solid #ddd;}
.zy_archives .cont .right_main .pieces .activity_list .top p b{display: block; font-weight: normal; margin-bottom: 6px;}
.zy_archives .cont .right_main .pieces .activity_list .top p em{display: block; font-style: normal;}
.zy_archives .cont .right_main .pieces .activity_list .top p:last-of-type b{color: #5093e1;}
.zy_archives .cont .right_main .pieces .activity_list .bottom{height: 371px; overflow-y: auto;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul{padding: 17px 26px 10px;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li{position: relative; margin-bottom: 15px;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li:before{position: absolute; top: 7px; left: -12px; display: block; content: ""; border: 3px solid #ccc; border-radius: 50%;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li p{box-sizing: border-box; width: 73%; padding-right: 20px;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li p span{display: block; text-overflow: ellipsis; overflow: hidden; white-space: nowrap;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li p em{font-style: normal; color: #666; font-size: 12px;}
.zy_archives .cont .right_main .pieces .activity_list .bottom ul li b{box-sizing: border-box; font-weight: normal; width: auto; height: 24px; line-height: 24px; padding: 0 6px; font-size: 12px; color: #5093e1; border: 1px solid #5093e1;}
