.recorded_date{height: 20px;line-height: 20px;margin-bottom: 25px;}
.recorded_date span{font-size: 14px;}
.recorded_date span:first-child{margin-right: 18px;}
.recorded_info li{width: 176px;margin-right: 38px;margin-bottom:12px;float: left;}
.recorded_info li .recorded_video{width: 176px;height: 113px;background: skyblue;display: block;cursor: pointer;}
.recorded_title{width: 100%;font-size: 16px;line-height: 24px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;margin-top: 5px;}
.recorded_time{font-size: 14px;color: #4f5355;line-height: 24px;margin-top: 5px;}
.recordedvideo_content{height: 698px;}
.recordedvideo_info{height:66px;line-height:66px;padding: 0 18px;font-size: 14px;color: #333;border-bottom:1px solid #e5e5e5;}
.recordedvideo_show{padding: 0 20px;margin-top: 22px;}
.recordedvideo_video{width: 100%;height: 566px;background: skyblue;}
.recordedvideo_count{font-size: 14px;color: #666;background:url("../img/zmj_eye.png") left center no-repeat;background-size: 20px 12px;padding-left: 26px;margin-top: 12px;float: right;}
/*学生实践 zmj 2018-08-29*/
.studentpractice_list li{width: 476px;height: 42px;line-height: 42px;padding: 0 20px;background: #f1f2f3;border-radius: 2px;margin-right:24px;margin-bottom: 18px;float: left;cursor: pointer;}
.studentpractice_list li a{width: 100%;height: 100%;display: block;}
.studentpractice_list li span{font-size: 16px;color: #282a2c;}
.studentpractice_list li span:last-child{font-size: 14px;color: #4f5355;float: right;}
.studentpractice_list li b{font-weight: normal;}
.studentpractice_title{height:52px;line-height: 52px;padding: 0 28px;font-size: 14px;background: #fff;border-radius: 5px;}
.studentpractice_title span:last-child{font-size: 12px;color: #4f5355;float: right;}
.studentpractice_details{height: 828px;margin-top: 16px;}
.studentpractice_left{width: 73%;height: 100%;padding:20px;box-sizing:border-box;background: #fff;border-radius: 5px;float: left;}
.studentpractice_playvideo{width: 100%;height: 462px;background: skyblue;}
.studentpractice_commentfigure{font-size: 16px;color: #222;margin-top: 12px;}
.studentpractice_commentcase{width: 100%;background:#f1f2f3;border-radius: 2px;margin-top: 12px;position: relative;}
.studentpractice_commentcase:before{content: "";width: 0;border:8px solid transparent;border-bottom-color: #f1f2f3;position: absolute;top:-14px;left:40px;}
.studentpractice_commentlist{max-height: 300px;overflow-y: auto;}
.studentpractice_commentlist li{padding: 10px 22px;}
.studentpractice_commentlist li p{font-size: 12px;color: #666;}
.studentpractice_commentlist li div{font-size:16px;color: #222;padding-top: 4px;}
.studentpractice_commentlist li div span:first-child{color: #147ec9;}
.studentpractice_right{width: 25.6%;height:100%;padding: 0 12px;box-sizing:border-box;background: #fff;border-radius: 5px;overflow-y:auto;float:right;} 
.studentpractice_videogroup{margin-bottom: 10px;}
.studentpractice_groupname{height: 36px;line-height: 36px;font-size: 16px;color: #4f5355;}
.studentpractice_bigvideo{height: 113px;border:4px solid #ccc;border-radius: 5px;cursor: pointer;}
.studentpractice_videogroup p.active{color: #147ec9;}
.studentpractice_videogroup div.active{border-color:#5093e1;}
.studentpractice_videogather {height: 48px;padding: 8px 12px;background: #edf1f2;margin-top: 10px;}
.studentpractice_videogather div{width: 30%;height: 100%;background: skyblue;float: left;cursor: pointer;}
.studentpractice_videogather div:nth-child(2){margin: 0 5%;}

