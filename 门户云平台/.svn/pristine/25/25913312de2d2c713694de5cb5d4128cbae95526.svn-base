.zyx_testPaperTit span.zyx_lookCard:hover{color: #1379C1;}
.zyx_testPaperTit span i:before{display: inline-block;transform: rotateZ(0deg);transition: all .5s ease;}
.zyx_testPaperTit span.active>i.icon-arrow_you:before{transform: rotateZ(90deg);transition: all .5s ease;}
.zyx_testCard{position: relative;min-height: 750px;}
.zyx_testCard img{width: 100%;}
.zyx_testCard .zyx_testCradMark.active{width:100%;height:100%;position: absolute;left: 0px;top: 0px;background: rgba(0,0,0,0.3);z-index:1;}
.zyx_testCard .zyx_LookAnswerCard{height:100%;position: absolute;top: 0;right: 0;width: 500px;background: #fff;z-index: 2;}
.zyx_testCard .zyx_LookAnswerCard img{margin-bottom: 10px;}
.zyx_testCard .zyx_LookAnswerCard h3{height: 52px;line-height:52px;border-bottom: 1px solid #ddd; font-size: 16px;color: #4f5355;padding-left: 35px;}
.zyx_CorrectAnswer{box-sizing: border-box;padding: 14px 25px 30px  20px;line-height: 30px}
.zyx_CorrectAnswer>span,.zyx_CorrectAnswer>div.zyx_otherSpan>span{display: inline-block;background: #f1f2f3;margin: 0 10px;width: 32px;height: 32px;line-height: 32px;text-align: center;border-radius: 60%;font-weight: normal;}
.zyx_CorrectAnswer>span.active,.zyx_CorrectAnswer>div.zyx_otherSpan>span.active{background: #19BC9D;color: #fff;}
.zyx_CorrectAnswer>div.zyx_otherSpan{margin: 10px 0;}
.zyx_CorrectAnswer>div.zyx_otherSpan p{display: inline-block;}
.zyx_LookAnswerCard .zyx_topic{width: 92%;}


/*作业-学生情况*/
.zyx_homeWork .zyx_lines{width: 796px;padding: 0;}
.zyx_homeWork .zyx_refer{background: #399fdf;}
.zyx_conrect{color:#7F8487;}
.zyx_postil{margin-top: 30px;}
.zyx_postil b{font-weight: normal; float: left;margin-top: 15px;}
.zyx_postil b:last-child{width: 94%;display: inline-block;}
.zyx_postil p{margin-bottom: 8px;margin-left: 6px;}
.zyx_postil textarea{width: 100%;resize: none;border: 1px solid #ddd; border-radius: 5px; box-sizing: border-box; padding: 5px;}
.zyx_myAnswer .zyx_postil textarea{width: 90%;}
.zy_student_resource_main .zyx_testPaperTit{width: 812px;height: 50px;border:none;}
.zy_student_resource_main .zyx_Showtype{padding: 16px;box-sizing: border-box;}
/*.zy_student_resource_main .zyx_Showtype .zyx_LookAnswerCard{height: auto;}*/

/*作业--未提交/已提交/已批阅*/
.zyx_testName{height: 50px;line-height: 50px;padding-left: 40px;background: #fff;border-bottom: 1px solid #ddd;font-size: 16px;}
.zyx_Overall{background: #fff; padding: 20px; border-radius: 5px;}
.zyx_Overall p{line-height: 30px;}
.homework_unsubmitted .zyx_testCard{border: 1px solid #ddd;width: 1198px;}
.homework_unsubmitted .zyx_testCard .zyx_testPaperTit>i{cursor: pointer;font-size: 18px;}
.homework_unsubmitted .zyx_testCard.active{width:700px;}
.homework_unsubmitted .zyx_EditAnswerCard{width: 480px;background: #fff;border: 1px solid #ddd;}
.homework_unsubmitted .zyx_EditAnswerCard .zyx_AddQuestion>div{padding-bottom: 0;}
.homework_unsubmitted .zyx_EditAnswerCard .zyx_topic{width: 94%;}
.homework_unsubmitted .zyx_EditAnswerCard .zyx_notLearn{width: 95%;}
.homework_unsubmitted .zyx_EditAnswerCard .zyx_notLearn .zyx_fiveSmallTopic_AnswerLine>div{width: 90%;}
.homework_unsubmitted .zyx_EditAnswerCard .zyx_myAnswer{margin: 10px;}
.homeword_submission .zyx_testCard .zyx_LookAnswerCard,.homeword_haveRead   .zyx_testCard .zyx_LookAnswerCard{height: auto;}
.MarginTopTen{margin-top: 18px;}
.zyx_ResourceBar{line-height: 30px; width: 340px; height: 30px;background: #e2f1fa;box-sizing: border-box; padding: 0 12px;font-size: 12px;margin-bottom: 10px;}
.zyx_ResourceBar>span{display: inline-block;width: 270px;overflow: hidden; text-overflow: ellipsis; white-space: nowrap;background:  #e2f1fa;}
.zyx_ResourceBar>img{margin-top: 6px;}

.zyx_studenBotton{position: relative;top: 30px;}
