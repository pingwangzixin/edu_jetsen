								<div class="zyx_topic" ng-repeat='i in echoQuestion.material'>
									<!--展示-->
									<div class="zyx_LookShow toggle">
										<div class="zyx_Top clearfix">
											<div class="tit clearfix">
												<h5 class="fl"><b ng-bind='$index+1'>1</b>材料题</h5>
