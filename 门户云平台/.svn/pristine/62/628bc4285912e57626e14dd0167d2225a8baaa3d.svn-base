<div class="main_width">
	<div class="zy_student_main_wrap clearfix">
		<!--左：三种类型-->
		<div class="zy_student_pass_wrap fl">
			<div class="zyx_AddResources_left">
				<!--卷库展示-->
				<div class="zyx_testpaperBox">
					<!--试卷标题-->
					<div class="zyx_testPaperTit">
						<span ng-bind="variablePacket.stuAssemblyName">
							<!--刘敏的作业-->  
						</span>
					</div>
					<!--8种题型展示-->
					<div class="zyx_new_edit">
						<!--题型展示-->
						<div class="zyx_AddQuestion">
							<!--单选-->
							<div class="zyx_Single" ng-if='variablePacket.eightSwitchOut[0].show' ng-attr-id="{{'single'}}">
								<p class="zyx_topicHead">一、单选题</p>
								<div class="zyx_topic" ng-repeat='i in questionBank.Out.single'>
									<!--展示-->
									<div class="zyx_LookShow toggle">
										<div class="zyx_Top clearfix">
											<div class="tit clearfix">
												<h5 class="fl"><b ng-bind='$index+1'>1</b>单选题</h5>
											</div>
											<div class="queTit" ng-bind-html='i.queTit | trustHtml'></div>
											<div class="fl zyx_myAnswer zyx_learn" ng-if="variablePacket.stuId!=''">
												<em>学生答案:</em>
												<span class="zyx_SingleBg" ng-bind-html='i.myAnswer | trustHtml'></span>
												<i class="iconfont icon-dui" ng-if='i.icon=="correct"'></i>
												<i class="iconfont icon-cuo" ng-if='i.icon=="error"'></i>
												<i class="iconfont icon-bandui" ng-if='i.icon=="half"'></i>
											</div>
											<div class="zyx_father clearfix">
												<span class="linesRight fr" ng-click='lookAnswer(i.Type,$index,i.AnswerShow)' ng-class='{"active":i.AnswerShow}'>
														查看答案及解析  <i class="iconfont icon-arrow_you"></i>
												</span>
											</div>
										</div>
										<ul class="zyx_Bottom scale-fade" ng-if='i.AnswerShow'>
											<li class="Answer">
												<strong>答案</strong>
												<div ng-bind-html='i.Answer | trustHtml'></div>
											</li>
											<li class="Analysis">
												<strong>解析</strong>
												<div ng-bind-html='i.Analysis | trustHtml'></div>
											</li>
											<!--<li class="Resource" ng-if='i.CanResource'>
												<strong>资源</strong>
												<div class="clearfix">
													<img ng-src = "{{i.Resource[0].ResourceSrc|icon}}" />
													<span ng-bind='i.Resource[0].ResourceTit'>班级空间我的班我的班的班我的班我的班级空间jpg</span>
												</div>
											</li>-->
										</ul>
									</div>
								</div>
							</div>
	
							<!--判断题-->
							<div class="zyx_Judge" ng-if='variablePacket.eightSwitchOut[1].show' ng-attr-id="{{'judge'}}">
								<p class="zyx_topicHead">二、判断题</p>
								<div class="zyx_topic" ng-repeat='i in questionBank.Out.judge'>
									<!--展示-->
									<div class="zyx_LookShow toggle">
										<div class="zyx_Top clearfix">
											<div class="tit clearfix">
												<h5 class="fl"><b ng-bind='$index+1'>1</b>判断题</h5>
											</div>
											<div class="queTit" ng-bind-html='i.queTit | trustHtml'></div>
											<div class="fl zyx_myAnswer" ng-if="variablePacket.stuId!=''">
												<em>学生答案:</em>
												<span class="zyx_JudgeBg" ng-bind-html='i.myAnswer | trustHtml'></span>
												<i class="iconfont icon-dui" ng-if='i.icon=="correct"'></i>
												<i class="iconfont icon-cuo" ng-if='i.icon=="error"'></i>
												<i class="iconfont icon-bandui" ng-if='i.icon=="half"'></i>
											</div>
											<div class="zyx_father clearfix">
												<span class="linesRight fr" ng-click='lookAnswer(i.Type,$index,i.AnswerShow)' ng-class='{"active":i.AnswerShow}'>
														查看答案及解析  <i class="iconfont icon-arrow_you"></i>
												</span>
											</div>
										</div>
										<ul class="zyx_Bottom scale-fade" ng-if='i.AnswerShow'>
											<li class="Answer">
												<strong>答案</strong>
												<div ng-bind-html='i.Answer | trustHtml'></div>
											</li>
											<li class="Analysis">
												<strong>解析</strong>
												<div ng-bind-html='i.Analysis | trustHtml'></div>
											</li>
											<!--<li class="Resource" ng-if='i.CanResource'>
												<strong>资源</strong>
												<div class="clearfix">
													<img ng-src = "{{i.Resource[0].ResourceSrc|icon}}" />
													<span ng-bind='i.Resource[0].ResourceTit'></span>
												</div>
											</li>-->
										</ul>
									</div>
								</div>
							</div>
	
							<!--简答题-->
							<div class="zyx_BriefAnswer" ng-if='variablePacket.eightSwitchOut[2].show' ng-attr-id="{{'briefAnswer'}}" repeat-finish="renderFinish()">
								<p class="zyx_topicHead">三、简单题</p>
								<div class="zyx_topic" ng-repeat='i in questionBank.Out.briefAnswer'>
									<!--展示-->
									<div class="zyx_LookShow toggle">
										<div class="zyx_Top clearfix">
											<div class="tit clearfix">
												<h5 class="fl"><b ng-bind='$index+1'>1</b>简答题</h5>
											</div>
											<div class="queTit" ng-bind-html='i.queTit | trustHtml'></div>
											<div class="zyx_myAnswer" ng-if="variablePacket.stuId!=''">
												<em>学生答案:</em>
												<div class="zyx_OtherLine clearfix">
													<span class="zyx_OtherBg" ng-if="i.myAnswer!=''" ng-bind-html='i.myAnswer | trustHtml'></span>
													
													<ul class="zmj_uploadImg zmj_teacherImg clearfix fl" >
														<li ng-repeat="j in i.img">
														     <img ng-src="{{j}}" alt="" ng-click="lookImg($index)" />
														</li>	    
												    </ul>
													
													
													<i class="iconfont cursor icon-dui" ng-if="variablePacket.stuState==1"  ng-click='markExam($event, $index, "correct")'></i>
													<i class="iconfont cursor icon-cuo"  ng-if="variablePacket.stuState==1" ng-click='markExam($event, $index, "error")'></i>
													<i class="iconfont cursor icon-bandui" ng-if="variablePacket.stuState==1" ng-click='markExam($event, $index, "half")'></i>
													
													<i class="iconfont icon-dui" ng-if='variablePacket.stuState==2&&i.icon=="correct"'></i>
													<i class="iconfont icon-cuo" ng-if='variablePacket.stuState==2&&i.icon=="error"'></i>
													<i class="iconfont icon-bandui" ng-if='variablePacket.stuState==2&&i.icon=="half"'></i>
												</div>
												<div class="zmj_teacherPostil">
													<b>批注：</b><input type="text" />
												</div>
											</div>
											<div class="zyx_father clearfix">
												<span class="linesRight fr" ng-click='lookAnswer(i.Type,$index,i.AnswerShow)' ng-class='{"active":i.AnswerShow}'>
														查看答案及解析  <i class="iconfont icon-arrow_you"></i>
												</span>
											</div>
										</div>
										<ul class="zyx_Bottom scale-fade" ng-if='i.AnswerShow'>
											<li class="Answer">
												<strong>答案</strong>
												<div ng-bind-html='i.Answer | trustHtml'></div>
												<!--<div ng-repeat='s in briefAnswerArr[$index]  track by $index'>
													<span ng-bind='"("+($index+1)+")"'></span>
													<span ng-bind='s'></span>
												</div>-->
												
											</li>
											<li class="Analysis">
												<strong>解析</strong>
												<div ng-bind-html='i.Analysis | trustHtml'></div>
											</li>
											<!--<li class="Resource" ng-if='i.CanResource'>
												<strong>资源</strong>
												<div class="clearfix">
													<img ng-src = "{{i.Resource[0].ResourceSrc|icon}}" />
													<span ng-bind='i.Resource[0].ResourceTit'></span>
												</div>
											</li>-->
										</ul>
									</div>
								</div>
							</div>
	
						</div>
					</div>
					
					<!--保存按钮-->
					<div class="zyx_SaveButton" ng-show="variablePacket.stuState==1">
						<span  ng-click='SaveButton()'>保存</span>
					</div>
					
					<!--返回顶部-->
					<!--<div class="zyx_blackTop" ng-click='zyx_blackTop()'>
						<i class="iconfont icon-huidaodingbu"></i>
						<p>返回顶部</p>
					</div>-->
					
				</div>
			</div>
		</div>
		<!--右：学生列表-->
		<div class="zy_student_wrap fr">
			<p>学生姓名</p>
			<div class="zy_inclass_stulist">
				<p>已提交</p>
				<ul class="clearfix">
					<li class="stay clearfix" ng-repeat="i in variablePacket.stuList.notRead">
						<strong class="fl" ng-bind="i.surname"></strong>
						<div class="fl">
							<em ng-bind="$index+1"></em>
							<span ng-bind="i.name"></span>
							<b ng-bind="i.state | checkState" ng-click="clickStu(i.id, i.name, i.state)"><!--待批阅--></b>
						</div>
					</li>
					<li class="already clearfix" ng-repeat="i in variablePacket.stuList.alreadyRead">
						<strong class="fl" ng-bind="i.surname"></strong>
						<div class="fl">
							<em ng-bind="$index+1"></em>
							<span ng-bind="i.name"></span>
							<b ng-bind="i.state | checkState" ng-click="clickStu(i.id, i.name, i.state)"><!--已批阅--></b>
						</div>
					</li>
				</ul>
				<p>未提交</p>
				<ul class="clearfix">
					<li class="not clearfix" ng-repeat="i in variablePacket.stuList.noSub">
						<strong class="fl" ng-bind="i.surname"></strong>
						<div class="fl">
							<em ng-bind="$index+1"></em>
							<span ng-bind="i.name"></span>
							<b ng-bind="i.state | checkState"><!--未提交--></b>
						</div>
					</li>
				</ul>
			</div>
			<!--<div class="zy_student_right_main">
				<div class="zy_student_list">
					<ul class="clearfix" ng-class="{'active':variablePacket.listBorder}">
						<li class="zy_not_read" ng-repeat="i in variablePacket.stuList.notRead">
							<span ng-bind="i.name"></span>
							<b ng-click="checkStuInfo(i.id, i.name, 'submission')">批阅</b>
						</li>
						<li class="zy_already_read" ng-repeat="i in variablePacket.stuList.alreadyRead">
							<span ng-bind="i.name"></span>
							<b ng-click="checkStuInfo(i.id, i.name, 'readyOver')">已批阅</b>
						</li>
						<li class="zy_no_sub" ng-repeat="i in variablePacket.stuList.noSub">
							<span ng-bind="i.name"></span>
						</li>
					</ul>
				</div>
			</div>-->
		</div>
	</div>  
	
	<!--图片查看弹框-->
		<div class="gy_del_down zmj_imgLook" ng-if="variablePacket.imgLookState">
		    <div class="gy_hide"></div>
		    <div class="gy_con">
		    	<div class="prev"><i class="iconfont icon-zuojiantoubeijing" ng-click="prev()" ng-class={'active':variablePacket.prevState}></i></div>
		    	<div class="imgshow">
		    		<img ng-src="{{variablePacket.imgSrc}}" alt="" />
		    	</div>
		        <div class="next"><i class="iconfont icon-youjiantoubeijing" ng-click="next()" ng-class={'active':variablePacket.nextState}></i></div>
		        <i class="iconfont icon-guanbi closes" ng-click="close()"></i>
			</div>
		</div>
	<!--确认框及提示框-->
	<prompt-box variable-packet="variablePacket" prompt-show="promptShow" wran-show="wranShow" del-ok="delOk"></prompt-box>
</div>

