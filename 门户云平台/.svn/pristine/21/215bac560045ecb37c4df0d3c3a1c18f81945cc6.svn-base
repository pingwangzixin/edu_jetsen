												<div class="zy_small_question_analysis_box_edit">
													<p>解析</p>
													<div class="zyx_setHeight">
														<div class="ueditor" ng-model='bbbb'></div>
													</div>
												</div>
											</div>
											<div class="zy_small_question_edit_btn">
												<button>确认</button>
												<button ng-click="questionBank.single[$index].edit=false">取消</button>
											</div>
										</div>
										<!--可编辑部分-->
									</li>
								</ul>
							</div>	
