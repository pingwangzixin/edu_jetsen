html{font:normal normal 62.5% "Microsoft YaHei";color:#222222;}/*加上是--1rem等于10px*/
body {
    font-family: "微软雅黑", Arial, sans-serif;
}
.handType{cursor:pointer;}
.navcenter {
    width: 100%;
    margin: 0 auto;
    background: #FFF;
    padding: 15px 15px 22px;
    overflow: hidden;
    box-sizing:border-box;background:#edf1f2;
}
.navcenter .navcenter-biaoti {
    width: 100%;
    height: 20px;
}
.navcenter .navcenter-biaoti span {
    float: left;
    font-size: 1.4rem;height:18px;
    color: #333333;
    line-height:14px;
    cursor: pointer;border-bottom:2px solid #5093e1;
}
.navcenter .navcenter-biaoti span.fanhui {
    float: right;
    color: #137DC9;
    width: 62px;
    text-align: center;
    background: url("../img/return-bg.png") no-repeat center left;
}
.navcenter .navcenter-biaoti span.fanhui a {
    color: #137DC9;
}
.navcenter .curriculum{background:#fff;}
.navcenter .curriculum .about {width: 100%;height:42px;line-height: 50px;padding:4px 22px 0;box-sizing:border-box;}
.navcenter .curriculum .about input {padding-left:16px;height: 33px;border-radius: 5px;border: none;outline:none;cursor: pointer;color: #333;background:url(../img/arrowL.png) no-repeat left center;color:#5093e1;font-size:1.2rem;}
.navcenter .curriculum .about input.nextweek {
    float: right;
    margin-top: 9px;padding-right:16px;background-position:right center;background-image:url(../img/arrowR.gif);
}
.navcenter .curriculum .formbiaoge {
    width: 100%;box-sizing:border-box;padding:0 20px 48px;
}
/*表格*/
table {
    *border-collapse: collapse; /* IE7 and lower */
    border-spacing: 0;
    width: 100%;
}
.bordered {
    border: solid #ccc 1px;
    -moz-border-radius: 6px;
    -webkit-border-radius: 6px;
    border-radius: 6px;
    -webkit-box-shadow: 0 1px 1px #ccc;
    -moz-box-shadow: 0 1px 1px #ccc;
    box-shadow: 0 1px 1px #ccc;
}
.bordered tr:hover {
    background: #fbf8e9;
    -o-transition: all 0.1s ease-in-out;
    -webkit-transition: all 0.1s ease-in-out;
    -moz-transition: all 0.1s ease-in-out;
    transition: all 0.1s ease-in-out;
}
.bordered td{height:58px;box-sizing:border-box;font-size:1.2rem;color:#333;}
.bordered td, .bordered th {
    border-left: 1px solid #ccc;
    border-top: 1px solid #ccc;
    padding: 10px;
    text-align: center;
    cursor: pointer;
}
.bordered td{padding:0 10px;}
.bordered td a span,.bordered td a p{font-size:1.2rem;}
.bordered td.current {
    border: 1px solid #ea3232;
}
.bordered td.current a {
    color: #ea3232;
}
.bordered td.active {
    border: 1px solid #00fd1e;
}
.bordered td.active a {
    color: #00fd1e;

}
.bordered th {
    height:46px;box-sizing:border-box;font-size:1.4rem;color:#333;
    box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
    border-top: none;
    text-shadow: 0 1px 0 rgba(255,255,255,.5);
}
.bordered td:first-child, .bordered th:first-child {
    border-left: none;
}
.bordered th:first-child {
    -moz-border-radius: 6px 0 0 0;
    -webkit-border-radius: 6px 0 0 0;
    border-radius: 6px 0 0 0;
}
.bordered th:last-child {
    -moz-border-radius: 0 6px 0 0;
    -webkit-border-radius: 0 6px 0 0;
    border-radius: 0 6px 0 0;
}
.bordered th:only-child{
    -moz-border-radius: 6px 6px 0 0;
    -webkit-border-radius: 6px 6px 0 0;
    border-radius: 6px 6px 0 0;
}
.bordered tr:last-child td:first-child {
    -moz-border-radius: 0 0 0 6px;
    -webkit-border-radius: 0 0 0 6px;
    border-radius: 0 0 0 6px;
}

.bordered tr:last-child td:last-child {
    -moz-border-radius: 0 0 6px 0;
    -webkit-border-radius: 0 0 6px 0;
    border-radius: 0 0 6px 0;
}

/*----------------------*/

.zebra td, .zebra th {
    padding: 10px;
    border-bottom: 1px solid #f2f2f2;
}

.zebra tbody tr:nth-child(even) {
    background: #f5f5f5;
    -webkit-box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
    -moz-box-shadow:0 1px 0 rgba(255,255,255,.8) inset;
    box-shadow: 0 1px 0 rgba(255,255,255,.8) inset;
}

.zebra th {
    text-align: left;
    text-shadow: 0 1px 0 rgba(255,255,255,.5);
    border-bottom: 1px solid #ccc;
    background-color: #eee;
    background-image: -webkit-gradient(linear, left top, left bottom, from(#f5f5f5), to(#eee));
    background-image: -webkit-linear-gradient(top, #f5f5f5, #eee);
    background-image: -moz-linear-gradient(top, #f5f5f5, #eee);
    background-image: -ms-linear-gradient(top, #f5f5f5, #eee);
    background-image: -o-linear-gradient(top, #f5f5f5, #eee);
    background-image: linear-gradient(top, #f5f5f5, #eee);
}

.zebra th:first-child {
    -moz-border-radius: 6px 0 0 0;
    -webkit-border-radius: 6px 0 0 0;
    border-radius: 6px 0 0 0;
}

.zebra th:last-child {
    -moz-border-radius: 0 6px 0 0;
    -webkit-border-radius: 0 6px 0 0;
    border-radius: 0 6px 0 0;
}

.zebra th:only-child{
    -moz-border-radius: 6px 6px 0 0;
    -webkit-border-radius: 6px 6px 0 0;
    border-radius: 6px 6px 0 0;
}

.zebra tfoot td {
    border-bottom: 0;
    border-top: 1px solid #fff;
    background-color: #f1f1f1;
}

.zebra tfoot td:first-child {
    -moz-border-radius: 0 0 0 6px;
    -webkit-border-radius: 0 0 0 6px;
    border-radius: 0 0 0 6px;
}

.zebra tfoot td:last-child {
    -moz-border-radius: 0 0 6px 0;
    -webkit-border-radius: 0 0 6px 0;
    border-radius: 0 0 6px 0;
}

.zebra tfoot td:only-child{
    -moz-border-radius: 0 0 6px 6px;
    -webkit-border-radius: 0 0 6px 6px
border-radius: 0 0 6px 6px;
}
/*选择时间*/
.navcenter .time {
    height:40px;background:#fff;border-radius:5px 5px 0  0;margin-top:15px;font-size:1.2rem;
}
.time {padding: 14px 0 0 20px;border-bottom:1px solid #ddd;}
.time .selectTime {display: inline-block;position: relative;line-height:30px;height:33px;}
.time .selectTime span{display:inline-block;}
.timeSelectFirst{float:left;}
.time input.datepicker {
    width: 170px;
    height: 30px;
    border-radius: 5px;
    border: 1px solid #ccc;
    padding-left: 20px;
    background: url("../img/magnifier.png") no-repeat center right;float:left;
}
.time input.submit {
    height: 30px;
    width:18px;
    position: absolute;
    top: 0;
    right: 84px;outline:none;border:none;
    cursor: pointer;background:url(../img/calender.png) no-repeat right center;
}
.time  .timeSure{width:60px;height:30px;line-height:30px;color:#fff;background:#5093e1;text-align:center;border-radius:5px;float:left;margin-left:20px;}